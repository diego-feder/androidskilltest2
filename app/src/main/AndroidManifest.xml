<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="diegofeder.testskill">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="diegofeder.testskill.ui.LoginActivity"
            android:label="@string/activity_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="diegofeder.testskill.ui.RegisterEditActivity"
            android:label="@string/activity_register">
        </activity>

        <activity
            android:name="diegofeder.testskill.ui.HomeActivity"
            android:label="@string/activity_home">
        </activity>

        <activity
            android:name="diegofeder.testskill.ui.AlbumsActivity"
            android:label="@string/activity_albums">
        </activity>

    </application>


</manifest>
